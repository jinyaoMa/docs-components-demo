@font-face
  font-family 'yui-icon'
  src url('./yui-icon.eot')
  src url('./yui-icon.eot?#iefix') format('embedded-opentype'), url('./yui-icon.woff') format('woff2'), url('./yui-icon.woff') format('woff'), url('./yui-icon.ttf') format('truetype')

[class^='yui-icon']
  font-family 'yui-icon'
  -moz-osx-font-smoothing grayscale
  -webkit-font-smoothing antialiased
  display inline-block
  font-style normal
  font-variant normal
  text-rendering auto
  line-height 1
  display inline-flex
